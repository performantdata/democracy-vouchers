{
  "name": "democracy-vouchers-static",
  "version": "1.0.0",
  "private": true,
  "description": "Static Web files for the Democracy Vouchers project.",
  "keywords": [],
  "homepage": "https://github.com/performantdata/democracy-vouchers/",
  "bugs": {
    "url": "https://github.com/performantdata/democracy-vouchers/issues/"
  },
  "license": "",
  "author": "Michael (https://github.com/performantdata)",
  "repository": {
    "type": "git",
    "url": "https://github.com/performantdata/democracy-vouchers.git"
  },
  "scripts": {
    "build": "run-s clean build:eleventy build:parcel",
    "build:eleventy": "npx @11ty/eleventy",
    "build:parcel": "parcel build --no-cache",
    "clean": "rm -rf target/web/public/main/ target/web/stage/",
    "start": "run-p start:eleventy start:parcel",
    "start:eleventy": "npx @11ty/eleventy --watch",
    "start:parcel": "parcel serve --target default --dist-dir target/web/stage/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@graphiql/plugin-explorer": "^1.0.3",
    "graphiql": "^3.1.1",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.11.0",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "svgo": "^2.8.0"
  },
  "targets": {
    "default": {
      "source": [
        "target/web/public/main/index.html",
        "target/web/public/main/candidate/index.html",
        "target/web/public/main/graphiql/index.html",
        "target/web/public/main/redeemed-vouchers/index.html",
        "target/web/public/main/voter-guide/index.html",
        "target/web/public/main/voter-guide/2026/index.html",
        "target/web/public/main/voucher/index.html"
      ],
      "distDir": "target/web/stage/"
    }
  }
}
